<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.Administrator" table="administrator">
		<id name="id" type="java.lang.Integer">
		<column name="administratorId"></column>
			<generator class="increment" />
		</id>
		<property name="password" column="administratorPassword" />
		<property name="name" column="administratorName" />	
	</class>
</hibernate-mapping>  
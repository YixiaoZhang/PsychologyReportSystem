<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.Instructor" table="instructor">
		<id name="id" type="java.lang.Integer">
		<column name="instructorId"></column>
			<generator class="increment" />
		</id>
		<property name="password" column="instructorPassword" />
		<property name="name" column="instructorName" />
		<property name="gradeId" column="gradeId" />		
	</class>
</hibernate-mapping>    
